{
  "name": "@gy/nestjs-observability",
  "version": "0.0.0",
  "description": "NestJs Observability Lib",
  "author": "Filipe Versehgi <filipeversehgi@gmail.com>",
  "license": "MIT",
  "readmeFilename": "README.md",
  "main": "dist/index.js",
  "files": [
    "dist/**/*",
    "*.md"
  ],
  "scripts": {
    "start:dev": "tsc -w",
    "prebuild": "yarn test",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "keywords": [
    "nestjs"
  ],
  "peerDependencies": {
    "@nestjs/common": "^7.0.0"
  },
  "dependencies": {
    "@opentelemetry/api": "~1.0.4",
    "@opentelemetry/api-metrics": "^0.27.0",
    "@opentelemetry/auto-instrumentations-node": "^0.27.1",
    "@opentelemetry/core": "~1.0.1",
    "@opentelemetry/exporter-jaeger": "~1.0.1",
    "@opentelemetry/exporter-prometheus": "^0.27.0",
    "@opentelemetry/host-metrics": "^0.27.0",
    "@opentelemetry/instrumentation-graphql": "^0.27.1",
    "@opentelemetry/propagator-b3": "~1.0.1",
    "@opentelemetry/propagator-jaeger": "~1.0.1",
    "@opentelemetry/sdk-node": "^0.27.0",
    "@opentelemetry/sdk-trace-base": "~1.0.1",
    "nestjs-pino": "~2.4.0",
    "opentelemetry-node-metrics": "~1.1.0",
    "pino": "^7.6.2",
    "pino-http": "^6.5.0",
    "response-time": "^2.3.2"
  },
  "devDependencies": {
    "@nestjs/common": "^7.0.0",
    "@nestjs/core": "^7.0.0",
    "@nestjs/platform-express": "^7.0.0",
    "@nestjs/testing": "6.1.1",
    "@types/express": "4.16.1",
    "@types/jest": "24.0.11",
    "@types/node": "11.13.4",
    "@types/pino-http": "^5.8.1",
    "@types/supertest": "2.0.7",
    "jest": "^27.0.0",
    "prettier": "^2.3.2",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.5.5",
    "supertest": "4.0.2",
    "ts-jest": "^27.0.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "^3.10.1",
    "tslint": "5.16.0",
    "typescript": "^4.3.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
